#! /usr/bin/env bash
exec /home/johnny/miniconda3/envs/digital/bin/vvp -f "$0" "$@" <<'# EOF'
:ivl_version "13.0 (devel)" "(0_8-6817-g77f7609b6-dirty)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/home/johnny/miniconda3/envs/digital/lib/ivl/system.vpi";
:vpi_module "/home/johnny/miniconda3/envs/digital/lib/ivl/vhdl_sys.vpi";
:vpi_module "/home/johnny/miniconda3/envs/digital/lib/ivl/vhdl_textio.vpi";
:vpi_module "/home/johnny/miniconda3/envs/digital/lib/ivl/v2005_math.vpi";
:vpi_module "/home/johnny/miniconda3/envs/digital/lib/ivl/va_math.vpi";
S_0x1b40000 .scope module, "test" "test" 2 1;
 .timescale 0 0;
v0x1b8b890_0 .var "clk", 0 0;
v0x1b8b960_0 .var "reset", 0 0;
v0x1b8ba30_0 .net "value", 7 0, v0x1b8b690_0;  1 drivers
S_0x1b40670 .scope module, "c1" "counter" 2 18, 3 1 0, S_0x1b40000;
 .timescale 0 0;
    .port_info 0 /OUTPUT 8 "out";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "reset";
P_0x1b3f230 .param/l "WIDTH" 0 3 3, +C4<00000000000000000000000000001000>;
v0x1b3f320_0 .net "clk", 0 0, v0x1b8b890_0;  1 drivers
v0x1b8b690_0 .var "out", 7 0;
v0x1b8b770_0 .net "reset", 0 0, v0x1b8b960_0;  1 drivers
E_0x1b7ad40 .event posedge, v0x1b8b770_0, v0x1b3f320_0;
    .scope S_0x1b40670;
T_0 ;
    %wait E_0x1b7ad40;
    %load/vec4 v0x1b8b770_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x1b8b690_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x1b8b690_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x1b8b690_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1b40000;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1b8b960_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1b8b890_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x1b40000;
T_2 ;
    %delay 17, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1b8b960_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1b8b960_0, 0, 1;
    %delay 29, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1b8b960_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1b8b960_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 10 "$stop" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x1b40000;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x1b8b890_0;
    %nor/r;
    %store/vec4 v0x1b8b890_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x1b40000;
T_4 ;
    %vpi_call 2 21 "$monitor", "At time %t, value = %h (%0d)", $time, v0x1b8ba30_0, v0x1b8ba30_0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_tb.v";
    "counter.v";
# EOF
