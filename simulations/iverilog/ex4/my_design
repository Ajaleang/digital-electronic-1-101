:vpi_module "/home/johnny/miniconda3/envs/digital/lib/ivl/vhdl_textio.vpi";
:vpi_module "/home/johnny/miniconda3/envs/digital/lib/ivl/v2005_math.vpi";
:vpi_module "/home/johnny/miniconda3/envs/digital/lib/ivl/va_math.vpi";
S_0xba7000 .scope module, "test" "test" 2 1;
 .timescale 0 0;
v0xbf78d0_0 .var "clk", 0 0;
v0xbf7970_0 .var "reset", 0 0;
v0xbf7a10_0 .net "value", 7 0, v0xbd0100_0;  1 drivers
v0xbf7b10_0 .var "value_old", 7 0;
S_0xba7670 .scope module, "dut" "counter" 2 23, 3 1 0, S_0xba7000;
 .timescale 0 0;
    .port_info 0 /OUTPUT 8 "out";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "reset";
P_0xbd05b0 .param/l "WIDTH" 0 3 3, +C4<00000000000000000000000000001000>;
v0xbd0060_0 .net "clk", 0 0, v0xbf78d0_0;  1 drivers
v0xbd0100_0 .var "out", 7 0;
v0xbf77b0_0 .net "reset", 0 0, v0xbf7970_0;  1 drivers
E_0xbe4190 .event posedge, v0xbf77b0_0, v0xbd0060_0;
    .scope S_0xba7670;
T_0 ;
    %wait E_0xbe4190;
    %load/vec4 v0xbf77b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0xbd0100_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0xbd0100_0;
    %addi 1, 0, 8;
    %assign/vec4 v0xbd0100_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0xba7000;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xbf7970_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xbf78d0_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0xbf7b10_0, 0, 8;
    %end;
    .thread T_1;
    .scope S_0xba7000;
T_2 ;
    %delay 17, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xbf7970_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xbf7970_0, 0, 1;
    %delay 29, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xbf7970_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xbf7970_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 12 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0xba7000;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0xbf78d0_0;
    %nor/r;
    %store/vec4 v0xbf78d0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0xba7000;
T_4 ;
    %vpi_call 2 27 "$monitor", "Time: %t, value = %h (%0d)", $time, v0xbf7a10_0, v0xbf7a10_0 {0 0 0};
    %end;
    .thread T_4;
    .scope S_0xba7000;
T_5 ;
    %vpi_call 2 33 "$dumpfile", "my_design.vcd" {0 0 0};
    %vpi_call 2 34 "$dumpvars", 32'sb00000000000000000000000000000000, S_0xba7000 {0 0 0};
    %end;
    .thread T_5;
    .scope S_0xba7000;
T_6 ;
    %wait E_0xbe4190;
    %load/vec4 v0xbf7970_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %flag_get/vec4 4;
    %jmp/0 T_6.2, 4;
    %load/vec4 v0xbf7a10_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/ne;
    %flag_get/vec4 4;
    %and;
T_6.2;
    %flag_set/vec4 8;
    %jmp/0xz  T_6.0, 8;
    %vpi_call 2 43 "$display", "Time: %t, error: Reset = %d, out = %d, se esperaba en out = 0", $time, v0xbf7970_0, v0xbf7a10_0 {0 0 0};
    %load/vec4 v0xbf7a10_0;
    %store/vec4 v0xbf7b10_0, 0, 8;
    %jmp T_6.1;
T_6.0 ;
    %load/vec4 v0xbf7b10_0;
    %pad/u 32;
    %addi 1, 0, 32;
    %load/vec4 v0xbf7a10_0;
    %pad/u 32;
    %cmp/ne;
    %jmp/0xz  T_6.3, 4;
    %load/vec4 v0xbf7b10_0;
    %pad/u 32;
    %addi 1, 0, 32;
    %vpi_call 2 49 "$display", "Time: %t, error: value: %d, se esperaba value = %d", $time, v0xbf7a10_0, S<0,vec4,u32> {1 0 0};
    %load/vec4 v0xbf7a10_0;
    %assign/vec4 v0xbf7b10_0, 0;
T_6.3 ;
T_6.1 ;
    %jmp T_6;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
